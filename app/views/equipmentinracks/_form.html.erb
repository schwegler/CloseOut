<%= semantic_form_for @equipmentinrack do |f| %>
  <%= f.inputs do %>
	<%= f.input :cabinet_id, :as => :select, :collection => Cabinet.all, :label_method => :format_name, :value_method => :id %>
    <%= f.input :site_id, :as => :select, :collection => Site.all, :label_method => :format_name, :value_method => :id %>
    <%= f.input :item %>
    <%= f.input :serialnumber %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>

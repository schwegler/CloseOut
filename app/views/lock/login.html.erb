<div class="jumbotron subhead" id="overview">
  <div class="container">
    <h1>Login</h1>
  </div>
</div>
<div class="container">
  <div class="page-header">
  <h1>What's the password?</h1></div>

<div id="lock-login-form">
  <%= form_tag unlock_url, :class => 'input-append' do %>
    <%=password_field_tag "password", nil, :class => 'span2', :id => 'appendedInputButton' %>
    <%=submit_tag "Unlock", :class => 'btn'%>
  <% end %>
</div>
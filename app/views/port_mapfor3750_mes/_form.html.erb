<%= semantic_form_for @port_mapfor3750_me do |f| %>
  <%= f.inputs do %>
    <%= f.input :cabinet_id, :as => :select, :collection => Cabinet.all, :label_method => :format_description, :value_method => :id %>
    <%= f.input :site_id, :as => :select, :collection => Site.all, :label_method => :format_name, :value_method => :id %>
    <%= f.input :port %>
    <%= f.input :connecteddevice %>
    <%= f.input :swport %>
    <%= f.input :patchpanelport %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
